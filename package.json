{
  "name": "nhk-program",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "parcel build index.html --bundle-node-modules --no-source-maps --public-url ./ --experimental-scope-hoisting",
    "release": "electron-builder --publish never",
    "start": "electron .",
    "test": "jest",
    "test:watch": "jest --watch",
    "watch": "parcel index.html"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^23.3.13",
    "@types/node": "^10.12.18",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "electron": "^4.0.1",
    "electron-builder": "^20.38.5",
    "jest": "^23.6.0",
    "parcel": "^1.11.0",
    "ts-jest": "^23.10.5",
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "@material-ui/core": "^3.9.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "build": {
    "directories": {
      "output": "releases"
    },
    "mac": {},
    "files": [
      "./*",
      "./dist/*",
      "!**/node_modules/**"
    ]
  }
}

{
  "name": "nhk-program",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "parcel build index.html --bundle-node-modules --no-source-maps --public-url ./ --experimental-scope-hoisting",
    "release": "electron-builder --publish never",
    "start": "electron .",
    "test": "jest",
    "test:watch": "jest --watch",
    "watch": "parcel index.html"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/node": "^12.6.2",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "electron": "^5.0.6",
    "electron-builder": "^21.0.15",
    "jest": "^24.8.0",
    "parcel": "^1.12.3",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "@material-ui/core": "^4.2.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "build": {
    "directories": {
      "output": "releases"
    },
    "mac": {},
    "files": [
      "./*",
      "./dist/*",
      "!**/node_modules/**"
    ]
  }
}
